{
	"databases": [
		{
		"zodb1": {
			"storage": "ZODB",
			"path": "data/Data.fs",
			"configuration": {
                "pool_size": 100,
                "cache_size": 100
           	}
		}
	}],
	"host": "127.0.0.1",
	"port": 8080,
	"static": [
		{"favicon.ico": "static/favicon.ico"}
	],
	"root_user": {
		"password": "root"
	},
	"cors": {
		"allow_origin": ["*"],
		"allow_methods": ["GET", "POST", "DELETE", "HEAD", "PATCH"],
		"allow_headers": ["*"],
		"expose_headers": ["*"],
		"allow_credentials": true,
		"max_age": 3660
	},
	"utilities": []
}
